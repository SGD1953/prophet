{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"inlineSourceMap": false,
		"allowJs": true,
		"sourceMap": true,
		"outDir": "../out",
		"noImplicitAny": false,
		"removeComments": false,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedLocals": true
	}
}
